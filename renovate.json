{
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "all non-major dependencies",
      "groupSlug": "all-minor-patch"
    },
    {
      "matchPackageNames": [
        "ru.yandex.clickhouse:clickhouse-jdbc",
        "org.testcontainers:clickhouse"
      ],
      "enabled": false
    }
  ]
}
